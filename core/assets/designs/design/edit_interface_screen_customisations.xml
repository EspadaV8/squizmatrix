<contents>
	<section name="New Customisation" read_access="write">
		<field>
			<display_name />
			<note />
			<function_call>
				<paint_function   name="paintNewCustomisation"   />
				<process_function name="processNewCustomisation" />
			</function_call>
		</field>
	</section>
	<section name="Current Customisations">
		<field>
			<display_name />
			<note />
			<function_call>
				<paint_function name="paintCurrentCustomisations" />
			</function_call>
		</field>
	</section>
</contents>
