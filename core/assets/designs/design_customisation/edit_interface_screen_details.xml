<contents>
	<section name="Details">
		<field attribute="id_name">
			<display_name>Id Name</display_name>
			<note />
			<text width="60" height="1" />
		</field>
	</section>
	<section name="Design Areas">
		<field format="new_line">
			<display_name>Customised</display_name>
			<note/>
			<function_call>
				<paint_function   name="paintCustomisedDesignAreas"   />
				<process_function name="processCustomisedDesignAreas" />
			</function_call>
		</field>
		<field format="new_line">
			<display_name>Un-Customised</display_name>
			<note/>
			<function_call>
				<paint_function   name="paintUnCustomisedDesignAreas"   />
				<process_function name="processUnCustomisedDesignAreas" />
			</function_call>
		</field>
	</section>
	<section name="Customise Associated Files" read_access="write">
		<field>
			<display_name>New</display_name>
			<note>Uploading a tar archive (.tar) or a gzipped tar archive (.tag.gz or .tgz) will result in those archives being extracted and their files being placed in the files directory</note>
			<function_call>
				<paint_function   name="paintNewFiles"   />
				<process_function name="processNewFiles" />
			</function_call>
		</field>
	</section>
</contents>
