<contents>
	<section name="Details" write_access="write:attributes" limbo_access="1">
		<field attribute="name">
			<display_name>File Name</display_name>
			<note />
			<text width="25" height="1" />
		</field>
		<field>
			<display_name>Upload File</display_name>
			<note />
			<function_call>
				<paint_function		name="paintFileUpload" />
				<process_function	name="processFileUpload" />
			</function_call>
		</field>
	</section>
	<section name="" write_access="write:attributes" limbo_access="1">
		<field>
			<display_name></display_name>
			<note></note>
			<function_call>
				<paint_function		name="paintCreateEditor" />
				<process_function	name="processCreateEditor" />
			</function_call>
		</field>
	</section>
</contents>
