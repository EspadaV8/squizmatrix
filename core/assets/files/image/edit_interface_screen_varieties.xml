<contents>
	<section name="Current Varieties" write_access="write:attributes" limbo_access="0" show_if="hasVarieties">
		<function_call>
			<paint_function   name="paintCurrentVarieties"   />
			<process_function name="processCurrentVarieties" />
		</function_call>
	</section>
	<section name="New Variety" write_access="write:attributes" read_access="write:attributes" limbo_access="0">
		<function_call>
			<paint_function   name="paintNewVariety"   />
			<process_function name="processNewVariety" />
		</function_call>
	</section>
</contents>
