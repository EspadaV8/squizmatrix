<contents>
	<section name="Details" write_access="write:attributes">
		<field attribute="name">
			<display_name>Schema Name</display_name>
			<note />
			<text width="20" height="1" />
		</field>
	</section>
	<section name="Steps Defined For This Workflow" write_access="write:links">
		<field format="blank">
			<display_name></display_name>
			<note />
			<function_call>
				<paint_function   name="paintSteps"   />
				<process_function name="processSteps" />
			</function_call>
		</field>
	</section>
	<section name="Create New Step" read_access="write:links" write_access="write:links">
		<field>
			<display_name></display_name>
			<note />
			<function_call>
				<paint_function   name="paintNewStep"   />
				<process_function name="processNewStep" />
			</function_call>
		</field>
	</section>
</contents>