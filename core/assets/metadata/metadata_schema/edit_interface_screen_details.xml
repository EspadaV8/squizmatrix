<contents>
	<section name="Details" write_access="write:attributes">
		<field attribute="name">
			<display_name>Schema Name</display_name>
			<note />
			<text width="12" height="1" />
		</field>
	</section>
	<section name="Sections Defined For This Metadata Schema">
		<field>
			<display_name></display_name>
			<note />
			<function_call>
				<paint_function   name="paintSections"   />
				<process_function name="processSections" />
			</function_call>
		</field>
	</section>
	<section name="Create New Section" read_access="write:content" write_access="write:content">
		<field>
			<display_name></display_name>
			<note />
			<function_call>
				<paint_function   name="paintNewSection"   />
				<process_function name="processNewSection" />
			</function_call>
		</field>
	</section>
	<section name="Regenerate Metadata Files" read_access="write:content" write_access="write:content">
		<field>
			<display_name></display_name>
			<note />
			<function_call>
				<paint_function   name="paintRegenerate"   />
				<process_function name="processRegenerate" />
			</function_call>
		</field>
	</section>
</contents>