<contents>
	<section name="Details" write_access="write:attributes">
		<field attribute="name">
			<text width="12" height="1" />
		</field>
	</section>
	<section name="Sections Defined For This Metadata Schema" write_access="write:links">
		<field>
			<function_call>
				<paint_function   name="paintSections"   />
				<process_function name="processSections" />
			</function_call>
		</field>
	</section>
	<section name="Create New Section" read_access="write:links" write_access="write:links">
		<field>
			<function_call>
				<paint_function   name="paintNewSection"   />
				<process_function name="processNewSection" />
			</function_call>
		</field>
	</section>
	<section name="Regenerate Metadata Files" read_access="write:" write_access="write:">
		<field>
			<function_call>
				<paint_function   name="paintRegenerate"   />
				<process_function name="processRegenerate" />
			</function_call>
		</field>
	</section>
	<section name="XML Namespace Settings" read_access="write:" write_access="write:">
		<field>
			<function_call>
				<paint_function   name="paintDefaultXMLns"   />
				<process_function name="processDefaultXMLns" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintNamespace"   />
				<process_function name="processNamespace" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintNsURL"   />
				<process_function name="processNsURL" />
			</function_call>
		</field>
	</section>
	<section name="Options" write_access="write:attributes">
		<field attribute="frontend">
			<boolean true_text="yes" false_text="no" />
		</field>
	</section>
</contents>