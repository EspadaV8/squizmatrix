<contents>
	<section name="Details">
		<field attribute="name">
			<display_name>Schema Name</display_name>
			<note />
		</field>
	</section>
	<section name="Sections Defined For This Metadata Schema">
		<field>
			<display_name></display_name>
			<note />
			<function_call>
				<paint_function   name="paintSections"   />
				<process_function name="processSections" />
			</function_call>
		</field>
	</section>
	<section name="Create New Section">
		<field>
			<display_name></display_name>
			<note />
			<function_call>
				<paint_function   name="paintNewSection"   />
				<process_function name="processNewSection" />
			</function_call>
		</field>
	</section>
	<section name="Regenerate Metadata Files">
		<field>
			<display_name></display_name>
			<note />
			<function_call>
				<paint_function   name="paintRegenerate"   />
				<process_function name="processRegenerate" />
			</function_call>
		</field>
	</section>
	<section name="XML Namespace Settings">
		<field>
			<display_name></display_name>
			<note />
			<function_call>
				<paint_function   name="paintDefaultXMLns"   />
				<process_function name="processDefaultXMLns" />
			</function_call>
		</field>
		<field>
			<display_name>Namespace</display_name>
			<note />
			<function_call>
				<paint_function   name="paintNamespace"   />
				<process_function name="processNamespace" />
			</function_call>
		</field>
		<field>
			<display_name>Namespace URI</display_name>
			<note />
			<function_call>
				<paint_function   name="paintNsURL"   />
				<process_function name="processNsURL" />
			</function_call>
		</field>
	</section>
	<section name="Options">
		<field attribute="frontend">
			<display_name>Show On Frontend</display_name>
			<note />
		</field>
	</section>
</contents>