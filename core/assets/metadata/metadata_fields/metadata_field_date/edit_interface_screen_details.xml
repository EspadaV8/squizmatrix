<contents>
	<section name="Details" write_access="write:attributes">
		<field attribute="name">
			<text width="30" height="1" />
		</field>
		<field>
			<function_call>
				<paint_function   name="paintDefaultDateTime" />
				<process_function name="processDefaultDateTime" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintOffset" />
				<process_function name="processOffset" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintRepKeys" />
				<process_function name="processRepKeys" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintSetupParams" />
				<process_function name="processSetupParams" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintDateFormatKeys" />
				<process_function name="processDateFormatKeys" />
			</function_call>
		</field>
		<field attribute="format">
			<text width="15" height="1" />
		</field>
		<field attribute="scheme">
			<text width="30" height="1" />
		</field>
		<field attribute="language">
			<text width="30" height="1" />
		</field>
	</section>
	<section name="Options" write_access="write:attributes">
		<field attribute="editable">
			<boolean true_text="Yes" false_text="No" />
		</field>
		<field attribute="required">
			<boolean true_text="Yes" false_text="No" />
		</field>
		<field attribute="frontend">
			<selection style="list" />
		</field>
	</section>
</contents>