<contents>
	<section name="Send Cacheable Headers Settings">
		<function_call>
			<paint_function name="paintCacheableHeadersSettings" />
        </function_call>
	</section>
	<section name="Root URLs to Send Cacheable Headers">
		<field>
			<function_call>
				<paint_function name="paintCacheableHeadersRootURLs" />
				<process_function name="processCacheableHeadersRootURLs" />
            </function_call>
            <display_name />
			<note>Choose the system root URLs for which cacheable header should be sent regardless of other Matrix cache settings</note>			
		</field>
	</section>	
</contents>
