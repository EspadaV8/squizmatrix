<contents>
	<section name="Unrestricted Access" read_access="admin:" write_access="admin:security">
		<field>
			<display_name />
			<note />
			<function_call>
				<paint_function   name="paintUnrestricted"   />
				<process_function name="processUnrestricted" />
			</function_call>
		</field>
	</section>
	<section name="Restrict Access to Asset Types" read_access="admin:" write_access="admin:security" show_if="isRestricted">
		<field>
			<display_name />
			<note />
			<function_call>
				<paint_function   name="paintTypeRestrictions"   />
				<process_function name="processTypeRestrictions" />
			</function_call>
		</field>
	</section>
</contents>
