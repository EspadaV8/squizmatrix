<contents>
	<section name="Edit Graph">
		<field>
			<display_name></display_name>
			<note />
			<function_call>
				<paint_function   name="paintGraphDetails"   />
				<process_function name="processGraphDetails" />
			</function_call>
		</field>
	</section>
	<section name="Width And Height">
		<field attribute="width">
			<display_name>Width</display_name>
			<note />
		</field>
		<field attribute="height">
			<display_name>Height</display_name>
			<note />
		</field>
	</section>
	<section name="Axes Colors">
		<field attribute="x_axis_color">
			<display_name>x-Axis Color</display_name>
			<note />
		</field>
		<field attribute="y_axis_color">
			<display_name>y-Axis Color</display_name>
			<note />
		</field>
		<field attribute="x_font_color">
			<display_name>x-Axis Font Color</display_name>
			<note />
		</field>
		<field attribute="y_font_color">
			<display_name>y-Axis  Font Color</display_name>
			<note />
		</field>
	</section>
	<section name="Axes Titles And Intervals">
		<field attribute="x_set_title">
			<display_name>Set Titles For x-Axis?</display_name>
			<note />
		</field>
		<field attribute="y_set_title">
			<display_name>Set Titles For y-Axis?</display_name>
			<note />
		</field>
		<field attribute="x_set_interval">
			<display_name>Set Label Intervals For x-Axis?</display_name>
			<note />
		</field>
		<field attribute="y_set_interval">
			<display_name>Set Label Intervals For y-Axis</display_name>
			<note />
		</field>
	</section>
	<section name="Line And Border Colors">
		<field attribute="line_color">
			<display_name>Line Color</display_name>
			<note />
		</field>
		<field attribute="border_color">
			<display_name>Border Color</display_name>
			<note />
		</field>
	</section>
	<section name="Fill Colors And Gradients">
		<field attribute="fill_color">
			<display_name>Add A New Fill Color</display_name>
			<note />
		</field>
		<field>
			<display_name>Fill Colors</display_name>
			<note />
			<function_call>
				<paint_function name="paintFillColor"    />
				<process_function name="processFillColor"/>
			</function_call>
		</field>
		<field attribute="fill_grad_type">
			<display_name>Fill Gradient Type</display_name>
			<note>This creates a scaled fillstyle with colors flowing gradiently between 2 specified RGB values. You can choose to have either a fill color or a Gradient type.</note>
		</field>
		<field attribute="fill_gradient_min">
			<display_name>Gradient Color 1</display_name>
			<note />
		</field>
		<field attribute="fill_gradient_max">
			<display_name>Gradient Color 2</display_name>
			<note />
		</field>
	</section>
	<section name="Background Colors And Gradients">
		<field attribute="background_color">
			<display_name>Background Color</display_name>
			<note />
		</field>
		<field attribute="back_grad_type">
			<display_name>Gradient Type</display_name>
			<note>This creates a scaled fillstyle with colors flowing gradiently between 2 specified RGB values</note>
		</field>
		<field attribute="back_gradient_min">
			<display_name>Gradient Color 1</display_name>
			<note />
		</field>
		<field attribute="back_gradient_max">
			<display_name>Gradient Color 2</display_name>
			<note />
		</field>
	</section>
	<section name="Styles And Markers">
		<field attribute="line_style">
			<display_name>Choose A Line Style</display_name>
			<note />
		</field>
		<field attribute="value_marker">
			<display_name>Choose Value To be Marked On The Graph</display_name>
			<note>You can choose to display the value of an axis on the graph. Alternatively You can leave it blank to have no dataet marked.</note>
		</field>
		<field attribute="general_marker">
			<display_name>Choose The Type Of Marker</display_name>
			<note>You can choose to display one/more markers for dataset instead of the value. Alternatively You can leave it blank to have no dataet marked.</note>
		</field>
		<field attribute="marker_type">
			<display_name>Choose The type Of Marker For The Value</display_name>
			<note />
		</field>
		<field attribute="marker_format">
			<display_name>Choose The DataPreProcessing Format</display_name>
			<note>Data preprocessor used for preformatting a data.</note>
		</field>
		<field attribute="format_value">
			<display_name>Enter The Value Of The Format</display_name>
			<note>Please note that this should be a valid value for the respective Data PreProcessing Format. You need not enter any
				value here for 'Textual Representation of Numbers' and 'Roman Representation Of Numbers'.
			</note>
		</field>
		<field attribute="marker_background">
			<display_name>Choose The Fill Color For The Markers</display_name>
			<note />
		</field>
	</section>
</contents>
