<contents>
	<section name="Details">
		<field write_access="write:attributes" attribute="name">
			<text width="15" />
		</field>
	</section>
	<section name="Confirmation Button Names" write_access="write:attributes" >
		<field attribute="confirm_button_name">
			<text width="35" height="1" />
		</field>
		<field attribute="cancel_button_name">
			<text width="35" height="1" />
		</field>
	</section>
	<section name="Payment Gateway">
		<field write_access="write:attributes"  attribute="gateway_assetid">
			<assetid type_codes="payment_gateway:D" />
		</field>
	</section>
	<section name="Taxation" write_access="write:attributes">
		<field attribute="tax_name">
			<text width="10" height="1" />
		</field>
		<field attribute="tax_value">
			<int width="3" />
		</field>
	</section>
	<section name="Asset Purchase" write_access="write:attributes" >
		<field attribute="item_request_var_name">
			<text width="20" height="1" />
		</field>
		<field attribute="asset_price_source">
			<text width="40" height="1" />
		</field>
		<field attribute="asset_taxable_source">
			<text width="40" height="1" />
		</field>
		<field attribute="asset_name_source">
			<text width="40" height="1" />
		</field>
	</section>
	<section name="Generate Report">
		<field>
			<function_call>
				<paint_function	 name="paintExport" />
				<process_function name="processExport" />
			</function_call>
		</field>
	</section>
</contents>
