<contents screen="details">
	<section name="Details">
		<field attribute="name" />
		<field>
			<display_name>Product Price</display_name>
			<function_call>
				<paint_function   name="paintProductPrice" />
				<process_function name="processProductPrice" />
			</function_call>
		</field>
	</section>
	<section name="Product Description">
		<field attribute="description" />
		<field attribute="short_description" />
		<field attribute="details" />
	</section>
</contents>