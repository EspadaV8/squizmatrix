<contents>
	<section name="Details">
		<field>
			<display_name>Id Name</display_name>
			<note />
			<function_call>
				<paint_function name="paintIdName" />
			</function_call>
		</field>
	</section>
	<section name="Form" write_access="write:attributes">
		<field attribute="cart">
			<display_name>Cart</display_name>
			<note />
			<assetid type_codes="ecommerce_cart:D" />
		</field>
		<field attribute="ecom_form">
			<display_name>E-Commerce Form</display_name>
			<note />
			<assetid type_codes="page_custom_form_ecommerce:D" />
		</field>
		<field attribute="show_price">
			<display_name>Show Total</display_name>
			<note />
			<boolean true_text="Yes" false_text="No" />
		</field>
	</section>
</contents>