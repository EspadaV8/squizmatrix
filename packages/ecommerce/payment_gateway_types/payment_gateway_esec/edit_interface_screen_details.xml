<contents>
	<section name="Details" write_access="write:attributes">
		<field attribute="name">
			<text width="35" height="1" />
		</field>
		<field attribute="short_name">
			<text width="35" height="1" />
		</field>
	</section>
	<section name="Identifiers" write_access="write:attributes">
		<field attribute="merchant_id">
			<text width="35" height="1" />
		</field>
	</section>
	<section name="Testing Mode" write_access="write:attributes">
		<field attribute="test_mode">
			<boolean true_text="ON" false_text="OFF" />
		</field>
	</section>
	<section name="Message" write_access="write:attributes">
		<field attribute="message">
			<wysiwyg style="div" width="100%" height="100" />
		</field>
	</section>
</contents>
