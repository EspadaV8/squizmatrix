<contents>
	<section name="Details" write_access="write:attributes" limbo_access="1">
		<field attribute="name">
			<text width="35" height="1" />
		</field>
		<field attribute="short_name">
			<text width="35" height="1" />
		</field>
		<field attribute="ecom_form_id">
			<assetid type_codes="page_custom_form_ecommerce:D" />
		</field>
	</section>
	<section name="Display Details" write_access="write:attributes" limbo_access="1">
		<field attribute="show_price">
			<boolean true_text="yes" false_text="no" />
		</field>
		<field attribute="checkout_text">
			<text width="35" height="1" />
		</field>
	</section>
</contents>
