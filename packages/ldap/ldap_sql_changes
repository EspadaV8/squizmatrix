ALTER TABLE sq_rollback_asset_permission CHANGE COLUMN userid userid VARCHAR(140) NOT NULL DEFAULT '0';
ALTER TABLE sq_rollback_asset_permission ADD PRIMARY KEY(sq_effective_from, assetid, userid, permission);


