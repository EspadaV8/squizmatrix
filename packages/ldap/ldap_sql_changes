MYSQL - sq_asset_editing_access
----------------------------------
ALTER TABLE sq_asset_editing_access DROP PRIMARY KEY;
ALTER TABLE sq_asset_editing_access CHANGE COLUMN userid userid VARCHAR(140) NOT NULL DEFAULT '0';
ALTER TABLE sq_asset_editing_access CHANGE COLUMN type_code type_code VARCHAR(140) NOT NULL;
ALTER TABLE sq_asset_editing_access ADD PRIMARY KEY(userid, type_code, screen, section);

ALTER TABLE sq_rollback_asset_editing_access DROP PRIMARY KEY;
ALTER TABLE sq_rollback_asset_editing_access CHANGE COLUMN userid userid VARCHAR(140) NOT NULL DEFAULT '0';
ALTER TABLE sq_rollback_asset_editing_access CHANGE COLUMN type_code type_code VARCHAR(140) NOT NULL;
ALTER TABLE sq_rollback_asset_editing_access ADD PRIMARY KEY(sq_effective_from, userid, type_code, screen, section);


PSQL - sq_asset_editing_access
----------------------------------
ALTER TABLE sq_asset_editing_access ADD COLUMN temp VARCHAR(255);
UPDATE sq_asset_editing_access SET temp = userid;
ALTER TABLE sq_asset_editing_access DROP COLUMN userid;
ALTER TABLE sq_asset_editing_access RENAME COLUMN temp TO userid;
ALTER TABLE sq_asset_editing_access ALTER COLUMN userid SET DEFAULT '0';
ALTER TABLE sq_asset_editing_access ALTER COLUMN userid SET NOT NULL;

ALTER TABLE sq_asset_editing_access ADD PRIMARY KEY (userid, type_code, screen, section);


ALTER TABLE sq_rollback_asset_editing_access ADD COLUMN temp VARCHAR(255);
UPDATE sq_rollback_asset_editing_access SET temp = userid;
ALTER TABLE sq_rollback_asset_editing_access DROP COLUMN userid;
ALTER TABLE sq_rollback_asset_editing_access RENAME COLUMN temp TO userid;
ALTER TABLE sq_rollback_asset_editing_access ALTER COLUMN userid SET DEFAULT '0';
ALTER TABLE sq_rollback_asset_editing_access ALTER COLUMN userid SET NOT NULL;

ALTER TABLE sq_rollback_asset_editing_access ADD PRIMARY KEY (sq_effective_from, userid, type_code, screen, section);




MYSQL - sq_asset_permission
----------------------------------
ALTER TABLE sq_asset_permission DROP PRIMARY KEY;
ALTER TABLE sq_asset_permission CHANGE COLUMN assetid assetid VARCHAR(140) NOT NULL DEFAULT '0';
ALTER TABLE sq_asset_permission CHANGE COLUMN userid userid VARCHAR(140) NOT NULL DEFAULT '0';
ALTER TABLE sq_asset_permission ADD PRIMARY KEY(assetid, userid, permission);

ALTER TABLE sq_rollback_asset_permission DROP PRIMARY KEY;
