<contents>
	<section name="Details">
		<field attribute="name">
			<display_name>Page Name</display_name>
			<note />
		</field>
		<field attribute="short_name">
			<display_name>Page Short Name</display_name>
			<note />
		</field>
	</section>
	<section name="Map Settings">
		<field attribute="api_key">
			<display_name>API Key</display_name>
			<note>The API Key provided by Google</note>
		</field>
		<field attribute="map_control">
			<display_name>Map Control</display_name>
			<note>Select one of the available control types.</note>
		</field>
		<field attribute="scale_control">
			<display_name>Scale Control</display_name>
			<note>Enable or Disable the Zoom Control of the map.</note>
		</field>

		<field attribute="map_type_control">
			<display_name>Map Type Control</display_name>
			<note>Enable or Disable the Map Type Control of the map</note>
		</field>
		<field attribute="enable_listing">
			<display_name>Enable Listing</display_name>
			<note>Enable or Disable the Listing Capability.</note>
		</field>
		<field attribute="use_custom_popup_html">
			<display_name>Custom HTML popup</display_name>
			<note>Enable or Disable custom HTML popup</note>
		</field>
		<field attribute="enable_adding_marker">
			<display_name>Enable adding marker</display_name>
			<note>Allow adding of markers on the map</note>
		</field>
	</section>
	<section name="Default Settings">
		<field>
			<display_name></display_name>
			<note></note>
	        <function_call>
	            <paint_function name="paintDefaultLatLng" />
	            <process_function name="processDefaultLatLng" />
	        </function_call>
		</field>
        <field attribute="default_map_type">
            <display_name>Default Map Type</display_name>
        </field>
		<field>
			<display_name></display_name>
			<note></note>
	        <function_call>
	            <paint_function name="paintDefaultMarkerIcon" />
	            <process_function name="processDefaultMarkerIcon" />
	        </function_call>
		</field>
		<field>
			<display_name></display_name>
			<note></note>
	        <function_call>
	            <paint_function name="paintDefaultIconSize" />
	            <process_function name="processDefaultIconSize" />
	        </function_call>
		</field>
	</section>
</contents>
