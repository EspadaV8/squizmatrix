<contents>
	<section name="Details" write_access="write:attributes">
		<field attribute="name">
			<text width="35" height="1" />
		</field>
	</section>
	<section name="SOAP Configuration" write_access="write:attributes">
		<field attribute="soap_operation_asset">
			<assetid type_codes="soap_wsdl_operation:I" />
		</field>
		<field attribute="use_application_layout">
			<boolean true_text="yes" false_text="no" />
		</field>
		<field>
			<function_call>
				<paint_function name="paintAddForm" />
				<process_function name="processAddForm" />
			</function_call>
		</field>
	</section>
	<section name="Data Mappings" write_access="write:attributes">
		<field attribute="parameter_map" format="new_line">
			<parameter_map />
		</field>
	</section>
</contents>
