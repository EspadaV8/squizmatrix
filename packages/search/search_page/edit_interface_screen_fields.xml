<contents>
	<section name="Searchable asset types">
		<field>
			<function_call>
				<paint_function   name="paintCurrentAssetTypes"   />
				<process_function name="processCurrentAssetTypes" />
			</function_call>
		</field>
		<field read_access="write:contents">
			<function_call>
				<paint_function   name="paintAddAssetType"   />
				<process_function name="processAddAssetType" />
			</function_call>
		</field>
	</section>
	<section name="Logical Grouping">
		<field attribute="logic">
			<selection /> 
		</field>
	</section>
	<section name="Field Configuration">
		<field>
			<function_call>
				<paint_function name="paintFields" />
				<process_function name="processFields" />
			</function_call>
		</field>
	</section>
	<section name="Field Styles">
		<field attribute="use_picker">
			<boolean true_text="Yes" false_text="No" />
		</field>
		<field read_access="" write_access="write:attributes">
			<function_call>
				<paint_function name="paintStyles" />
				<process_function name="processStyles" />
			</function_call>
		</field>
	</section>
</contents>
