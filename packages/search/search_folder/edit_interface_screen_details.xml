<contents>
	<section name="Identification" write_access="write:attributes">
		<field attribute="name">
			<text width="30" height="1" />
		</field>
	</section>
	<section name="Search Settings" write_access="write:attributes">
		<field>
			<function_call>
				<paint_function   name="paintRoot"   />
				<process_function name="processRoot" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintPhrase"   />
				<process_function name="processPhrase" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintLogic"   />
				<process_function name="processLogic" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintStatus"   />
				<process_function name="processStatus" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintCurrentAssetTypes"   />
				<process_function name="processCurrentAssetTypes" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintAddAssetType"   />
				<process_function name="processAddAssetType" />
			</function_call>
		</field>
	</section>
</contents>