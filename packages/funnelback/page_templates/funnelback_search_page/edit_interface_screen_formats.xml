<contents>
	<section name="Default Formats"  limbo_access="0">
		<field>
			<function_call>
				<paint_function   name="paintDefaultFormatLinks"   />
			</function_call>
		</field>
	</section>
	<section name="Spelling Formats"  limbo_access="0">
		<field>
			<function_call>
				<paint_function   name="paintSpellFormatLinks"   />
			</function_call>
		</field>
	</section>
	<section name="Customised Type Formats" write_access="write:links"  limbo_access="0">
		<field>
			<function_call>
				<paint_function   name="paintTypeFormats"   />
				<process_function name="processTypeFormats" />
			</function_call>
		</field>
	</section>
</contents>
