<contents>
	<section name="Progress">
		<function_call>
			<paint_function name="paintProgress" />
			<process_function name="processProgress" />
		</function_call>
	</section>
	<section name="Details" write_access="write:attributes">
		<field attribute="name">
			<text width="30" height="1" />
		</field>
	</section>
	<section name="Recipients" write_access="write:attributes">
		<field>
			<function_call>
				<paint_function name="paintRecipients"   />
				<process_function name="processRecipients" />
			</function_call>
		</field>
	</section>
	<section name="Content Details" write_access="write:attributes">
		<function_call>
			<paint_function name="paintContentDetails" />
			<process_function name="processContentDetails" />
		</function_call>
	</section>
	<section name="Header Details" write_access="write:attributes">
		<function_call>
			<paint_function name="paintHeaderDetails" />
			<process_function name="processHeaderDetails" />
		</function_call>
	</section>
</contents>