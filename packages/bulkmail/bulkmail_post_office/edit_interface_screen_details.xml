<contents>
	<section name="Details" write_access="write:attributes">
		<field attribute="name">
			<text width="30" height="1" />
		</field>
	</section>
	<section name="Job Progress">
		<function_call>
			<paint_function name="paintJobQueue" />
			<process_function name="processJobQueue" />
		</function_call>
	</section>
	<section name="Server Details" write_access="write:attributes">
		<function_call>
			<paint_function name="paintServerDetails" />
			<process_function name="processServerDetails" />
		</function_call>
	</section>
	<section name="Header Details" write_access="write:attributes">
		<field attribute="from">
			<text width="30" height="1" />
		</field>
		<field attribute="subject">
			<text width="30" height="1" />
		</field>
		<field>
			<function_call>
				<paint_function name="paintHeaderDetails" />
				<process_function name="processHeaderDetails" />
			</function_call>
		</field>
	</section>
	<section name="Threshold Settings" write_access="write:attributes">
		<function_call>
			<paint_function name="paintThresholds" />
			<process_function name="processThresholds" />
		</function_call>
	</section>
	<section name="Peak Hour Setting" write_access="write:attributes">
		<field attribute="time_range_from">
			 <datetime min=" 00:00" max="23:59" use_picker="0" allow_period="1">
				<show>
					<h style="s" allow_null="1" />
					<i style="s" allow_null="1" />
				</show>
			</datetime>
		</field>
		<field attribute="time_range_to">
			 <datetime min=" 00:00" max="23:59" use_picker="0" allow_period="1">
				<show>
					<h style="s" allow_null="1" />
					<i style="s" allow_null="1" />
				</show>
			</datetime>
		</field>
	</section>
</contents>