<messages>
	<message type="bulkmail.preview">
		<subject>Bulkmail Previewed</subject>
		<body><![CDATA[<SQ_MSG_TAGLINE>%assetid%</SQ_MSG_TAGLINE>]]> has sent a preview email to %email_addr%</body>
	</message>
	<message type="bulkmail.job.finished">
		<subject>Bulkmail Job Finished</subject>
		<body>Mail successfully sent to %num_recip% recipients</body>
	</message>
	<message type="bulkmail.job.cancelled">
		<subject>Bulkmail Job Cancelled</subject>
		<body>Job cancelled by <![CDATA[<SQ_MSG_TAGLINE>%assetid%</SQ_MSG_TAGLINE>]]></body>
	</message>
</messages>
