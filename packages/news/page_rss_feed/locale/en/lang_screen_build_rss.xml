<contents>
	<section name="Keywords As Items">
		<field>
			<display_name>Keywords Selected</display_name>
			<note />
			<function_call>
				<paint_function   name="paintSelectedKeywords"   />
				<process_function name="processSelectedKeywords" />
			</function_call>
		</field>
	</section>
	<section name="Add A Keyword">
		<field>
			<display_name> Add Keyword</display_name>
			<note />
			<function_call>
				<paint_function name="paintKeywords"     />
				<process_function name="processKeywords" />
			</function_call>
		</field>
	</section>
	<section name="Add Channel And Image Info">
		<field>
			<display_name />
			<note> You have to add information to the channel or the image tags like the links, descriptions, urls etc. </note>
			<function_call>
				<paint_function name="paintChannelImage"     />
				<process_function name="processChannelImage" />
			</function_call>
		</field>
	</section>
</contents>
