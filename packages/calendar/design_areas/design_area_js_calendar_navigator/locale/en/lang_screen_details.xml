<contents>
	<section name="Details">
		<field>
			<display_name>Id Name</display_name>
			<note />
			<function_call>
				<paint_function name="paintIdName" />
			</function_call>
		</field>
	</section>
	<section name="Calendar Options">
		<field attribute="target_assetid">
			<display_name>Calendar Page</display_name>
			<note />
		</field>
		<field attribute="element_id">
			<display_name>Element ID</display_name>
			<note>This is used by HTML, CSS and JS. Make sure it is unique on the page.</note>
		</field>
		<field attribute="style_assetid">
			<display_name>Custom CSS File</display_name>
			<note>Can be left blank. If nothing is supplied, a default stylesheet will be used</note>
		</field>
	</section>
</contents>