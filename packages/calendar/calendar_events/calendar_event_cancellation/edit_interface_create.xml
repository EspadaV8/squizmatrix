<contents screen="details">
	<section name="Details" write_access="write:attributes">
		<field>
			<display_name>Occurrence to cancel</display_name>
			<function_call>
				<paint_function   name="paintDateTimeChooser"  />
				<process_function name="processDateTimeChooser" />
			</function_call>
		</field>		
		<field attribute="description">
			<display_name>Reason for cancellation</display_name>
			<note />
			<text width="40" height="3" />
		</field>
	</section>
</contents>