		<field attribute="short_name">
			<text width="25" height="1" />
		</field>
		<field>
			<function_call>
				<paint_function name="paintEventSources" />
				<process_function name="processEventSources" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function name="paintDaysFrom" />
				<process_function name="processDaysFrom" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function name="paintDaysTo" />
				<process_function name="processDaysTo" />
			</function_call>
		</field>
		<field attribute="day_filter">
			<selection />
		</field>
		<field attribute="day_heading_format">
			<text width="20" height="1" />
		</field>
		<field attribute="day_heading_tag">
			<selection />
		</field>
	</section>
</contents>
