<contents>
	<section name="Details" write_access="write:attributes" limbo_access="1">
		<field attribute="name">
			<text width="30" height="1" />
		</field>
	</section>

	<section name="Status">
		<field>
			<function_call>
				<paint_function		name="paintStatus" />
			</function_call>
		</field>
	</section>

	<section name="Import XML" write_access="write:attributes">
		<field>
			<function_call>
				<paint_function		name="paintImportXML" />
				<process_function	name="processImportXML" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function		name="paintExampleXMLFile" />
			</function_call>
		</field>
	</section>

	<section name="Configuration" write_access="write:attributes" show_if="isThesaurusEnabled">
		<field>
			<function_call>
				<paint_function		name="paintPrimaryRelation" />
				<process_function	name="processPrimaryRelation" />
			</function_call>
		</field>
	</section>

	<section name="Export XML" write_access="write:attributes" show_if="isThesaurusEnabled">
		<field>
			<function_call>
				<paint_function		name="paintExportXMLfile" />
				<process_function	name="processExportXMLfile" />
			</function_call>
		</field>
	</section>

</contents>