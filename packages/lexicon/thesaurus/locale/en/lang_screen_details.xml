<contents>
	<section name="Details">
		<field attribute="name">
			<display_name>Name</display_name>
			<note />
		</field>
	</section>

	<section name="Status">
		<field>
			<display_name>Thesaurus Status</display_name>
			<note />
			<function_call>
				<paint_function		name="paintStatus" />
			</function_call>
		</field>
	</section>

	<section name="Import XML">
		<field>
			<display_name>Upload File</display_name>
			<note />
			<function_call>
				<paint_function		name="paintImportXML" />
				<process_function	name="processImportXML" />
			</function_call>
		</field>
		<field>
			<display_name>Example File</display_name>
			<note />
			<function_call>
				<paint_function		name="paintExampleXMLFile" />
			</function_call>
		</field>
	</section>

	<section name="Configuration">
		<field>
			<display_name>Primary Relation</display_name>
			<function_call>
				<paint_function		name="paintPrimaryRelation" />
				<process_function	name="processPrimaryRelation" />
			</function_call>
		</field>
	</section>

	<section name="Export XML">
		<field>
			<display_name>XML format</display_name>
			<function_call>
				<paint_function		name="paintExportXMLfile" />
				<process_function	name="processExportXMLfile" />
			</function_call>
		</field>
		<field attribute="contents_changed">
			<display_name>Contents Changed</display_name>
			<note />
		</field>
	</section>

</contents>