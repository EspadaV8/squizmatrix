<contents>
	<section name="Locking / Editing">
	    <field>
			<display_name></display_name>
			<note></note>
			<function_call>
				<paint_function   name="paintLockMessage"   />
				<process_function name="processLockMessage" />
			</function_call>
		</field>
	</section>
	<section name="Question Options">
	    <field attribute="name">
			<display_name>Title</display_name>
			<note></note>
		</field>
		<field attribute="note">
			<display_name>Note</display_name>
			<note></note>
		</field>
		<field>
			<display_name>Options</display_name>
			<note>If &quot;Show as text box&quot; is not checked, fields will be displayed as a drop-down list</note>
			<function_call>
				<paint_function   name="paintInterface"   />
				<process_function name="processInterface" />
			</function_call>
		</field>
		<field attribute="minimum">
			<display_name>Minimum</display_name>
			<note></note>
		</field>
		<field attribute="maximum">
			<display_name>Maximum</display_name>
			<note></note>
		</field>
		<field attribute="default">
			<display_name>Default</display_name>
			<note></note>
		</field>
		<field attribute="extra">
			<display_name>Extras</display_name>
			<note>Provides a means of specifying CSS classes, javascript onClick functions or other INPUT tag attributes, eg class="yourCssClass"</note>
		</field>
	</section>
</contents>