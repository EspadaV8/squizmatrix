<contents>
	<section name="Enable / Disable Logging">
		<field attribute="log_submissions">
			<boolean true_text="yes" false_text="no" />
		</field>
	</section>
	<section name="Export" write_access="write:attributes">
		<function_call>
			<paint_function   name="paintExport"   />
			<process_function name="processExport" />
		</function_call>
	</section>
	<section name="Submissions">
		<function_call>
			<paint_function   name="paintSubmissions"   />
			<process_function name="processSubmissions" />
		</function_call>
	</section>
</contents>