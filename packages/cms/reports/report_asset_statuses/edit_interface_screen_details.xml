<contents>
	<section name="Details" write_access="write:attributes" limbo_access="1">
		<field attribute="name">
			<text width="35" height="1" />
		</field>
		<field>
			<function_call>
				<paint_function   name="paintRootNode"  />
				<process_function name="processRootNode" />
			</function_call>
		</field>
		<field attribute="search_status">
			<selection style="list" height="1" />
		</field>
		<field>
			<function_call>
				<paint_function   name="paintLastStatusChange" />
				<process_function name="processLastStatusChange" />
			</function_call>
		</field>
		<field>
			<function_call>
				<paint_function   name="paintRegenerate"   />
				<process_function name="processRegenerate" />
			</function_call>
		</field>
	</section>
</contents>
