<contents>
	<section name="Details"  write_access="write:attributes">
	    <field attribute="name">
			<text width="30" height="1" />
		</field>
	    <field attribute="short_name">
			<text width="30" height="1" />
		</field>
	</section>
	<section name="Create Status"  write_access="write:attributes">
		<field attribute="create_status">
			<selection />
		</field>
	</section>			
	<section name="Template Stucture">
		<field>
			<function_call>
				<paint_function   name="paintQuestionFields"   />
				<process_function name="processQuestionFields" />
			</function_call>
		</field>
	</section>
</contents>