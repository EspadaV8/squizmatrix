<contents>
	<section name="Details" write_access="write:attributes" limbo_access="1">
		<field attribute="name">
			<text width="35" height="1" />
		</field>
		<field attribute="short_name">
			<text width="25" height="1" />
		</field>
	</section>
	<section name="HCL Schemas List" write_access="write:links">
		<field>
			<function_call>
				<paint_function   name="paintHCLSchemas"  />
				<process_function name="processHCLSchemas" />
			</function_call>
		</field>
		<field read_access="write:links">
			<function_call>
				<paint_function   name="paintAddSchema"  />
				<process_function name="processAddSchema" />
			</function_call>
		</field>
	</section>
	<section name="Additional Properties">
		<field write_access="write:links">
			<function_call>
				<paint_function   name="paintRootNode"  />
				<process_function name="processRootNode" />
			</function_call>
		</field>
		<field attribute="type" write_access="write:attributes">
			<selection style="list" />
		</field>
	</section>
</contents>
