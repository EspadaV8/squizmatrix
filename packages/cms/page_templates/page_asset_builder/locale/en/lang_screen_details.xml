<contents>
	<section name="Details">
		<field attribute="name">
			<display_name>Page Name</display_name>
			<note />
		</field>
		<field attribute="short_name">
			<display_name>Page Short Name</display_name>
			<note />
		</field>
	</section>
	<section name="Asset Builder Settings">
		<field>
			<display_name>Asset Types to Create</display_name>
			<note></note>
			<function_call>
				<paint_function   name="paintAssetTypes"   />
				<process_function name="processAssetTypes" />
			</function_call>
		</field>
		<field>
			<display_name>Create Status</display_name>
			<note />
			<function_call>
				<paint_function   name="paintCreateStatus"   />
				<process_function name="processCreateStatus" />
			</function_call>
		</field>
		<field attribute="allow_public_user">
			<display_name>Allow Public Users</display_name>
			<note>If you are putting the create form in the not-logged-in bodycopy, enable this option and link permissions will not be checked when creating the asset</note>
		</field>
	</section>
</contents>