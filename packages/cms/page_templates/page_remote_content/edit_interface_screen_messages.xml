<contents>
	<section name="Unavailable Content" write_access="write:attributes">
		<field attribute="display_error">
			<boolean true_text="Yes" false_text="No" />
		</field>		
		<field attribute="error_message">
			<wysiwyg style="div" width="100%" height="100" />
		</field>
	</section>
	<section name="Binary Files" write_access="write:attributes">
		<field attribute="binary_message">
			<wysiwyg style="div" width="100%" height="100" />
		</field>
	</section>
</contents>
